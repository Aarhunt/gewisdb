{
    "name": "gewis/gewisdb",
    "description": "GEWIS Database management",
    "license": "GPL-3.0",
    "keywords": [
        "members",
        "decisions",
        "gewis"
    ],
    "homepage": "http://gewis.nl/",
    "require": {
        "php": "^7.1.0",
        "ext-intl": "*",
        "ext-pdo_pgsql": "*",
        "ext-pgsql": "*",
        "ext-zend-opcache": "*",
        "ext-zip": "^1.12.0",
        "zendframework/zend-authentication": "^2.7.0",
        "zendframework/zend-config": "^3.3.0",
        "zendframework/zend-console": "^2.8.0",
        "zendframework/zend-crypt": "^3.3.1",
        "zendframework/zend-eventmanager": "^3.2.1",
        "zendframework/zend-form": "^2.4.13",
        "zendframework/zend-http": "^2.11.2",
        "zendframework/zend-i18n": "^2.10.1",
        "zendframework/zend-inputfilter": "^2.10.1",
        "zendframework/zend-mail": "^2.10.0",
        "zendframework/zend-mime": "^2.7.2",
        "zendframework/zend-modulemanager": "^2.8.4",
        "zendframework/zend-mvc": "^3.1.1",
        "zendframework/zend-mvc-i18n": "^1.1.1",
        "zendframework/zend-progressbar": "^2.7.0",
        "zendframework/zend-router": "^3.3.0",
        "zendframework/zend-session": "^2.4.13",
        "zendframework/zend-serializer": "^2.4.13",
        "zendframework/zend-servicemanager": "^3.4.0",
        "zendframework/zend-stdlib": "^3.2.1",
        "zendframework/zend-view": "^2.11.4",
        "doctrine/common": "^2.11.3",
        "doctrine/dbal": "^2.9.3",
        "doctrine/orm": "^2.7.5",
        "doctrine/doctrine-module": "^2.1.3",
        "doctrine/doctrine-orm-module": "^2.1.7",
        "monolog/monolog": "^1.27",
        "cweagans/composer-patches": "^1.7.2"
    },
    "require-dev": {
        "zendframework/zend-developer-tools": "^1.0.0",
        "phpunit/phpunit": "^5.7.27",
        "squizlabs/php_codesniffer": "^3.6.2",
        "friendsofphp/php-cs-fixer": "^2.19.3",
        "phpstan/phpstan": "^1.4.10",
        "phpstan/extension-installer": "^1.1.0",
        "bnf/phpstan-psr-container": "^1.0.1",
        "phpstan/phpstan-doctrine": "^1.2.11",
        "maglnet/composer-require-checker": "^2.0.0"
    },
    "config": {
        "preferred-install": {
            "doctrine/orm": "source",
            "*": "dist"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "laminas/laminas-dependency-plugin": true,
            "cweagans/composer-patches": true,
            "laminas/laminas-component-installer": true,
            "phpstan/extension-installer": true,
            "icanhazstring/composer-unused": true
        },
        "discard-changes" : true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "patches": {
            "doctrine/orm": {
                "Fix issues with SubDecisions.": "https://raw.githubusercontent.com/GEWIS/orm/2.12.x/1-to-1-multiple-join-columns.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src",
            "Checker\\": "module/Checker/src",
            "Database\\": "module/Database/src",
            "Report\\": "module/Report/src",
            "User\\": "module/User/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CheckerTest\\": "module/Checker/test"
        }
    }
}
